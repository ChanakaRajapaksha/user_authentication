generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
    id           Int      @id @default(autoincrement())
    username     String   @unique
    email        String   @unique
    password     String
    refreshToken String?  // Make sure this is defined as a single optional field
    createdAt    DateTime @default(now())
    updatedAt    DateTime @updatedAt
}

